{
  "name": "medusa-plugin-hyperswitch",
  "version": "0.0.2",
  "description": "A MedusaJS plugin for integrating with hyperswitch",
  "author": "Mahesh Vagicherla (https://maheshvagicherla.dev)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "build:admin": "cross-env medusa-admin build",
    "prepare": "cross-env NODE_ENV=production npm run build:server && medusa-admin bundle"
  },
  "dependencies": {
    "@juspay-tech/hyperswitch-node": "^1.0.4",
    "@medusajs/medusa": "^1.20.9",
    "@medusajs/ui": "^4.0.1",
    "add": "^2.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "^4.17.2",
    "medusa-interfaces": "1.3.10",
    "yarn": "^1.22.22",
    "@medusajs/admin": "^7.1.16"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.20.9",
    "react": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "files": [
    "dist",
    "build"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@stdlib/number-float64-base-normalize": "0.0.8",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mime": "1.3.5",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "resolutions": {
    "glob": "^7.1.6"
  }
}